		"composer-plugin-api": "*",
		"friendsofphp/php-cs-fixer": "^2.15"
	},
	"require-dev": {
		"composer/composer": "~1.0"
	},
	"autoload": {
		"psr-4": {
			"Onceyoungs\\PhpCsFixer\\": "src/"
